{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "azdevdatafactory"
		},
		"azblob_stg_employees_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azblob_stg_employees'"
		},
		"tgt_azuresqldatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'tgt_azuresqldatabase'"
		},
		"GetEmployees_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://dummy.restapiexample.com/api/v1/employees"
		},
		"srcRestAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://dummy.restapiexample.com/api/v1/employees"
		},
		"src_omdb_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://www.omdbapi.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azblob_stg_employees_connectionString": {
						"value": "[parameters('azblob_stg_employees_connectionString')]"
					},
					"tgt_azuresqldatabase_connectionString": {
						"value": "[parameters('tgt_azuresqldatabase_connectionString')]"
					},
					"GetEmployees_properties_typeProperties_url": {
						"value": "[parameters('GetEmployees_properties_typeProperties_url')]"
					},
					"srcRestAPI_properties_typeProperties_url": {
						"value": "[parameters('srcRestAPI_properties_typeProperties_url')]"
					},
					"src_omdb_api_properties_typeProperties_url": {
						"value": "[parameters('src_omdb_api_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}